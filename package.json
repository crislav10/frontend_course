  "bugs": {
    "url": "https://github.com/crislav10/frontend_course/issues"
  },
  "homepage": "https://github.com/crislav10/frontend_course#readme",
  "devDependencies": {
    "lite-server": "^2.6.1",
    "node-sass": "^6.0.1",
    "onchange": "^7.1.0",
    "parallelshell": "^3.0.2"
  },
  "dependencies": {
    "bootstrap": "^4.0.0",
    "bootstrap-social": "^5.1.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1"
  }
}
